search.grid-search
    form(action='aplicar a la vista')
        fieldset
            p Filtros:
            span 
            label Precio ascendente: 
                input(type='radio' name='ordenar_precio' value='radio1')
            label Precio descendente: 
                input(type='radio' name='ordenar_precio' value='radio2')
            div Marca:
                select#marca
                    option(value='value1') Marca 1
                    option(value='value2') Marca 2
                    option(value='value3') Marca 3
article.grid-layout-content
    each producto in productos
        .grid-box 
            img.imagen-teclado(src='/public/Images/teclado.avif' alt='Icono de rss')
            p.modelo-teclado #{producto.modelo}
            a.enlace-teclado #{producto.enlace}
            p.precio-teclado #{producto.precio}
